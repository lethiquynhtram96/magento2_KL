<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="header-wrapper">
            <container name="before.header" label="before-header" htmlTag="div" htmlClass="header before" before="-"/>
            <container name="header" label="header" htmlTag="div" htmlClass="header " after="before.header"/>
            <container name="menu" label="menu" htmlTag="div" htmlClass="menu" after="header"/>
  

        </referenceContainer>

    </body>
</page>
